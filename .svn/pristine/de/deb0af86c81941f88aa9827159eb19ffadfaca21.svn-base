            approveDate ,
            hasDeal ,
            dateCreate ,
            fileId
          )
		SELECT  flowId ,
				oaNo ,
				#{approveStatus} ,
				GETDATE(),
				0,
				GETDATE() ,
				#{newId}
		FROM dbo.PMLS_JsStatement where oaNo = (select t.oaNo from PMLS_JY_Apply a,
										PMLS_JsStatement t
										where a.jssid=t.id and a.jyapplyNo = #{jyNo}) and delFlag = 0;

		INSERT INTO  dbo.OaPmlsJsStatementFileReturn
