    //退款时间
    private Date refundDate;
    //门店地址
    private String AddressDetail;
    //Add by WangLei 2017/04/07 Start
    private String originalContractNo;
    
    private String agreementNo;
    
    private Integer agreementType;
    
    private BigDecimal  depositFee;
