<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.eju.pmls.rptCompany.dao.RptCompanyDetailMapper">

  <!--获取经纪公司明细列表-->
  <select id="queryCompanyDetailList"  parameterType="Map" resultType="cn.com.eju.pmls.rptCompany.dto.RptCompanyDetailDto">
    	

		--当年大定金额
        SELECT  companyNo ,
                SUM(yearlyAmount) thisTotal
		INTO #rough_this
        FROM    dbo.RPT_CompanyDetail
        WHERE   roughYear = #{roughYear}  --大定数据年份
        GROUP BY companyNo;

		--海波龙数据
		SELECT DISTINCT
			c.id
            ,c.CityNo 
            ,c.CityName
            ,ref.qyCode 
            ,ref.qyName
            ,ref.areaCityCode
	    	,ref.areaCityName
	   	INTO #rough_hbl
        FROM    dbo.cloud_shy_fy_city_ref_new ref
                INNER JOIN dbo.USR_Group g WITH(NOLOCK) ON ref.cityGroupId = g.groupId
                INNER JOIN dbo.BAS_City c  WITH(NOLOCK) ON c.ID = g.cityId
        WHERE   year = #{organization}    --架构年份
                AND orgType = 'FYYJCS'
                AND cityGroupId NOT IN ( -2, -4, 1733 )

		--旗下门店数
		SELECT  cs.CompanyId ,
                COUNT(DISTINCT s.StoreNo) storeNums
		INTO #store_num
        FROM    dbo.CompanyStore cs
                INNER JOIN dbo.Store s WITH(NOLOCK) ON cs.StoreId = s.Id
                                            AND s.IsDelete = 0
        WHERE   cs.IsDelete = 0 and  cs.flag = 0
        GROUP BY cs.CompanyId;
        
        
        --获取最新框架信息
		SELECT  *
		INTO    #new_lfc
		FROM    ( SELECT    ROW_NUMBER() OVER ( PARTITION BY lfc.companyNo ORDER BY lfc.id DESC ) AS w ,
							lfc.companyNo ,
							lfc.contractNo,
							CONVERT(VARCHAR(10), lfc.dateLifeStart, 120) dateLifeStart ,
							CONVERT(VARCHAR(10), lfc.dateLifeEnd, 120) dateLifeEnd ,
							CONVERT(VARCHAR(10), lfc.approvePassDate, 120) approvePassDate ,
							uufc.userCode ,
							uufc.userName
				  FROM      LNK_FrameContract lfc
							LEFT JOIN dbo.USR_User uufc WITH ( NOLOCK ) ON uufc.userId = lfc.userIdCreate
																  AND uufc.delFlag = 'N'
				  WHERE     lfc.delFlag = 0
				) new_lfc
		WHERE   w = 1;  
		
<!--                   SELECT  MAX(dateCreate),companyNo -->
<!-- 					FROM    LNK_FrameContract -->
<!-- 					WHERE delFlag=0   -->
<!-- 				  GROUP BY companyNo -->

        --获取签框架信息-是否签框架
        SELECT  CASE WHEN lfc.companyNo >'' THEN 'yes'
                     ELSE 'no'
                END isFrameContract ,
                lfc.*

        INTO    #frameData
        FROM    dbo.Company m4
                LEFT JOIN #new_lfc lfc ON lfc.companyNo = m4.CompanyNo;


		SELECT
		 ROW_NUMBER() OVER(ORDER BY  t1.DateCreate desc) AS rowNum  
		,t2.* 
        ,t1.Id id
        ,t1.companyNo  comCompanyNo
        ,t1.CompanyName companyName 
        ,t1.brandType                   --业务类型
        ,brandTypeStr = ( CASE WHEN t1.brandType =29401
                                         THEN '分销'
							   WHEN t1.brandType =29402
                                         THEN '其他'
										 ELSE '-'
                                    END )
        ,t3.CityNo acCityNo             --业绩所在城市  CRM
        ,t3.CityName acCityName 
        ,t3.areaCityCode areaCityNo          --业绩归属城市  海波龙
        ,t3.areaCityName areaCityName 
        ,t3.qyCode regionCode           --业绩归属区域  海波龙
        ,t3.qyName regionName
        ,t4.isFrameContract                          --是否签框架
        ,t4.contractNo                               --框架合同编号
        ,t4.dateLifeStart frameDateStart             --框架开始日期
        ,t4.dateLifeEnd   frameDateEnd             --框架结束日期
        ,t4.approvePassDate frameApprovePassDate     --框架过审日期
        ,t4.userCode  frameCreateUserCode            --框架创建人
        ,t4.userName  frameCreateUserName
		,ISNULL(t5.storeNums,0) storeNums      --旗下门店数             
		,dbo.F_ClearNullMoney(ISNULL(t6.thisTotal,0)+ISNULL(t2.preAmount,0)) totalAmount--总累计
		,dbo.F_ClearNullMoney(t2.preAmount) preTotal--当年以前
		,dbo.F_ClearNullMoney(t6.thisTotal) thisTotal--当前新增
FROM    dbo.Company t1           
        LEFT JOIN dbo.RPT_CompanyDetail t2 WITH(NOLOCK) ON t1.CompanyNo = t2.CompanyNo and  roughYear=#{roughYear}    --大定数据  年份
        LEFT JOIN #rough_hbl t3 WITH(NOLOCK) ON t3.CityNo = t1.AcCityNo
        LEFT JOIN #frameData t4 WITH(NOLOCK) ON t4.companyNo = t1.CompanyNo
        LEFT JOIN #store_num t5 WITH(NOLOCK) ON t5.CompanyId = t1.Id
		LEFT JOIN #rough_this t6 WITH(NOLOCK) ON t6.companyNo=t1.CompanyNo
		
WHERE   t1.IsDelete = 0
<!-- 		归属城市 -->
	    <if test="areaCityCodeList != null">
			AND t3.areaCityCode IN
			<foreach collection="areaCityCodeList" item="item" index="index"
					 open="(" separator="," close=")">
				#{item}
			</foreach>
		</if>
<!-- 		所在城市 -->
		<if test="cityIdList != null">
			AND t3.id IN
			<foreach collection="cityIdList" item="item" index="index"
					 open="(" separator="," close=")">
				#{item}
			</foreach>
		</if>
<!-- 			业务类型 -->
	    <if test="brandType != null and brandType!=''">
            AND t1.brandType=#{brandType}
        </if>
        <!--         是否签框架 -->
        <if test="isFrameContract!=null and isFrameContract!='' ">
	      	AND t4.isFrameContract=#{isFrameContract}
	    </if>
<!-- 	    经纪公司 -->
        <if test="companyNo!=null and companyNo!=''">
	      <![CDATA[
  				AND (t1.companyNo LIKE '%${companyNo}%' or t1.companyName LIKE '%${companyNo}%')
  				]]>
	    </if>
	    ORDER BY  rowNum;

	    
	    
DROP TABLE #rough_this;
DROP TABLE #rough_pre;
DROP TABLE #store_num;
DROP TABLE #new_lfc;
DROP TABLE #frameData;

  </select>
  
