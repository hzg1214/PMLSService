	<select id="queryCityList" parameterType="Map" resultType="cn.com.eju.deal.dto.common.CityDto">
		SELECT
			lc.cityId,
			lc.cityNo,
			c.cityName
		FROM
			BAS_LinkProjectTraceCity lc
			JOIN BAS_City c ON lc.cityId = c.id
			AND lc.delFlag = 0
			AND lc.userCode =#{userCode}
	</select>
