package cn.com.eju.deal.open.model;

import java.io.Serializable;
import java.util.Date;

/**
 * OMS系统中门店Dto层
 * 
 * @author sunmm
 * @date 2016年8月20日 下午7:55:58
 */
public class OmsStoreDto implements Serializable {

	/**
	 * 序列化
	 */
	private static final long serialVersionUID = 1131359641001513476L;

	/**
	 * 门店联系人
	 */
	private String contactName;

	/**
	 * 门店联系人电话
	 */
	private String contactPhone;

	/**
	 * 门店录入人姓名
	 */
	private String userName;

	/**
	 * 门店录入人工号
	 */
	private String userCode;

	/**
	 * 门店录入日期
	 */
	private Date dateCreate;

	/**
	 * 门店店招(门店名称)
	 */
	private String storeName;

	/**
	 * 门店地址
	 */
	private String storeAddress;

	/**
	 * 区域编号
	 */
	private String districtNo;

	/**
	 * 门店所属行政区
	 */
	private String districtName;

	/**
	 * 获取门店联系人姓名
	 * 
	 * @return contactName 门店联系人姓名
	 */
	public String getContactName() {
		return contactName;
	}

	/**
	 * 设置门店联系人姓名
	 * 
	 * @param contactName
	 *            门店联系人姓名
	 */
	public void setContactName(String contactName) {
		this.contactName = contactName;
	}

	/**
	 * 获取门店联系人电话
	 * 
	 * @return contactPhone 门店联系人电话
	 */
	public String getContactPhone() {
		return contactPhone;
	}

	/**
	 * 设置门店联系人电话
	 * 
	 * @param contactPhone
	 *            门店联系人电话
	 */
	public void setContactPhone(String contactPhone) {
		this.contactPhone = contactPhone;
	}

	/**
	 * 获取门店录入人姓名
	 * 
	 * @return userName 门店录入人姓名
	 */
	public String getUserName() {
		return userName;
	}

	/**
	 * 设置门店录入人姓名
	 * 
	 * @param userName
	 *            门店录入人姓名
	 */
	public void setUserName(String userName) {
		this.userName = userName;
	}

	/**
	 * 获取门店录入人工号
	 * 
	 * @return userCode 门店录入人工号
	 */
	public String getUserCode() {
		return userCode;
	}

	/**
	 * 设置门店录入人工号
	 * 
	 * @param userCode
	 *            门店录入人工号
	 */
	public void setUserCode(String userCode) {
		this.userCode = userCode;
	}

	/**
	 * 获取门店录入日期
	 * 
	 * @return dateCreate 门店录入日期
	 */
	public Date getDateCreate() {
		return dateCreate;
	}

	/**
	 * 设置门店录入日期
	 * 
	 * @param dateCreate
	 *            门店录入日期
	 */
	public void setDateCreate(Date dateCreate) {
		this.dateCreate = dateCreate;
	}

	/**
	 * 获取门店店招
	 * 
	 * @return storeName 门店店招
	 */
	public String getStoreName() {
		return storeName;
	}

	/**
	 * 设置门店店招
	 * 
	 * @param storeName
	 *            门店店招
	 */
	public void setStoreName(String storeName) {
		this.storeName = storeName;
	}

	/**
	 * 获取门店地址
	 * 
	 * @return storeAddress 门店地址
	 */
	public String getStoreAddress() {
		return storeAddress;
	}

	/**
	 * 设置门店地址
	 * 
	 * @param storeAddress
	 *            门店地址
	 */
	public void setStoreAddress(String storeAddress) {
		this.storeAddress = storeAddress;
	}

	/**
	 * 获取区域编号
	 * 
	 * @return districtNo 区域编号
	 */
	public String getDistrictNo() {
		return districtNo;
	}

	/**
	 * 设置区域编号
	 * 
	 * @param districtNo
	 *            区域编号
	 */
	public void setDistrictNo(String districtNo) {
		this.districtNo = districtNo;
	}

	/**
	 * 获取门店所属行政区
	 * 
	 * @return districtName 门店所属行政区
	 */
	public String getDistrictName() {
		return districtName;
	}

	/**
	 * 设置门店所属行政区
	 * 
	 * @param districtName
	 *            门店所属行政区
	 */
	public void setDistrictName(String districtName) {
		this.districtName = districtName;
	}

}
