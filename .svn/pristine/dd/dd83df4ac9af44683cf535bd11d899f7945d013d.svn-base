	<select id="getGNBySelectOrgId" resultMap="BaseResultUserInfoMap" parameterType="java.lang.String">
		SELECT  T1.groupName
        FROM    USR_Group T1
        WHERE   T1.delFlag = 'N'
                AND T1.typeId = 7
                AND #{selectOrgId ,jdbcType=NVARCHAR} LIKE T1.orgId.ToString() + '%'
        UNION
        SELECT  T1.groupName
        FROM    USR_Group T1
        WHERE   T1.delFlag = 'N'
