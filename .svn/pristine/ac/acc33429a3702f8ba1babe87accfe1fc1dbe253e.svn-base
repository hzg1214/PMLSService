    <select id="mergeInsert" parameterType="cn.com.eju.pmls.jsStatement.model.PmlsJsStatementDtl">
    MERGE INTO dbo.PMLS_JsStatement_Dtl s1
    USING (SELECT #{id} AS id) AS s2
    ON s1.id = s2.id
    WHEN MATCHED THEN
    UPDATE SET s1.reportId = #{reportId},s1.customerName = #{customerName},s1.customerTel = #{customerTel}
               ,s1.buildingNo = #{buildingNo},s1.cxArea = #{cxArea},s1.cxAmount = #{cxAmount}
               ,s1.jsAmount = #{jsAmount},s1.kpAmount = #{kpAmount},s1.kpTaxAmount = #{kpTaxAmount}
               ,s1.kpTaxAmountAfter = #{kpTaxAmountAfter},s1.contractYdAmount = #{contractYdAmount}
               ,s1.projectCode = #{projectCode},s1.projectName = #{projectName}
               ,s1.jsStatementType = #{jsStatementType},s1.offSetFlag = #{offSetFlag}
               ,s1.serviceFeeDes = #{serviceFeeDes},s1.isGlobalControl = #{isGlobalControl}
               ,s1.hsCode = #{hsCode},s1.hsName = #{hsName},s1.dealDate = #{dealDate}
               ,s1.delFlag = '0' ,s1.dateUpdate = #{dateUpdate} ,s1.userIdUpdate = #{userIdUpdate}
    WHEN NOT MATCHED THEN
        INSERT (jssId,reportId,customerName,customerTel,buildingNo,cxArea,cxAmount
                ,jsbl,jsAmount,jyAmount,kpAmount,kpTaxAmount,kpTaxAmountAfter,contractYdAmount
                ,yjsAmount,projectCode,projectName,delFlag,dateCreate,userIdCreate
                ,dateUpdate,userIdUpdate,jsStatementType,offSetFlag,requestAmount,serviceFeeDes
                ,isGlobalControl,hsCode,hsName,dealDate)
         values (#{jssId},#{reportId},#{customerName},#{customerTel},#{buildingNo},#{cxArea},#{cxAmount}
                ,#{jsbl},#{jsAmount},#{jyAmount},#{kpAmount},#{kpTaxAmount},#{kpTaxAmountAfter},#{contractYdAmount}
                ,#{yjsAmount},#{projectCode},#{projectName},#{delFlag,jdbcType=BIT},#{dateCreate},#{userIdCreate}
                ,#{dateUpdate},#{userIdUpdate},#{jsStatementType},#{offSetFlag},#{requestAmount},#{serviceFeeDes}
                ,#{isGlobalControl},#{hsCode},#{hsName},#{dealDate});
  </select>

    <select id="getJsStatementFile" parameterType="java.util.Map"  resultType="cn.com.eju.deal.dto.common.FileRecordMainDto">
