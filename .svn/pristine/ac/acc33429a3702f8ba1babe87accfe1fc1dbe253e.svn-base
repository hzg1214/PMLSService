				CompanyId ,
				RefId ,
				FileId ,
				FileTypeId ,
				FileFullName ,
				FileSourceId ,
				Remark ,
				DateCreate ,
				UserCreate ,
				IsDelete ,
				fileNo ,
				picUrl ,
				fileSuffix ,
				fileAbbrUrl ,
				url50 ,
				fileUrl ,
