            contractNo,
            contractName,
            htedition,
            projectNo,
            projectName,
            companyNo,
            companyName,
            htOaNo,
            contractType,
            hsCode,
            hsName,
            khCode,
            khName,
            htDateStart,
            htDateEnd,
            incomeSubject,
            commissionMemo,
            commissionMemoRemark,
            rtnCommission,
            rtnCommissionMemo,
            htoapassDate,
            wsqyzt,
            developersCode,
            developersName,
            dateLifeStart,
            dateLifeEnd,
            commissionAmount,
            commissionRatio,
            fyjstj,
            fyjsbz,
            remark,
            approveState,
            validStatus,
            delFlag,
            userIdCreate,
            dateCreate
        ) VALUES(
            #{cityNo},
            #{contractNo},
            #{contractName},
            #{htedition},
            #{projectNo},
            #{projectName},
            #{companyNo},
            #{companyName},
            #{htOaNo},
            #{contractType},
            #{hsCode},
            #{hsName},
            #{khCode},
            #{khName},
            #{htDateStart},
            #{htDateEnd},
            #{incomeSubject},
            #{commissionMemo},
            #{commissionMemoRemark},
            #{rtnCommission},
            #{rtnCommissionMemo},
            #{htoapassDate},
            #{wsqyzt},
            #{developersCode},
            #{developersName},
            #{dateLifeStart},
            #{dateLifeEnd},
            #{commissionAmount},
            #{commissionRatio},
            #{fyjstj},
            #{fyjsbz},
